{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noImplicitReturns": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "removeComments": true,
    "outDir": "dist",
    "plugins": [],
    "types": ["jest"],
    "paths": {
      "@core/*": ["../core/*"],
      "tslib": ["./node_modules/tslib/tslib.d.ts"]
    }
  }
}
